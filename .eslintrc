{
    "extends": "airbnb",
    "rules": {
        "strict": 0,
        "arrow-body-style": 0,
        "no-unused-expressions": [
            "error",
            {
                "allowTernary": true
            }
        ],
        "no-console": 0,
        "react/jsx-uses-react": "error",
        "react/jsx-uses-vars": "error",
        "indent": [
            "error",
            4,
            {
                "SwitchCase": 1
            }
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "never"
        ],
        "spaced-comment": 0,
        "max-len": 0,
        "camelcase": 0,
        "comma-dangle": [
            "error",
            "never"
        ],
        "object-curly-spacing": 0,
        "no-nested-ternary": 0,
        "no-param-reassign": 0,
        "react/jsx-indent-props": [
            4,
            "tab"
        ],
        "no-underscore-dangle": 0
    }
}
